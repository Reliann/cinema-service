{
  "name": "cinema-service",
  "version": "1.0.0",
  "description": "This example is so cool.",
  "main": "cinemaWs.js",
  "scripts": {
    "build": "cd my-app && npm run build",
    "install-my-app": "cd my-app && npm install",
    "heroku-postbuild": "npm run install-my-app && npm run build",
    "server": "nodemon cinemaWs.js",
    "develop": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix my-app\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix my-app\""
  },
  "keywords": [
    "example",
    "heroku",
    "express"
  ],
  "author": "rell",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonfile": "^6.1.0",
    "mongoose": "^6.1.3"
  },
  "engines": {
    "node": "14.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reliann/cinema-service.git"
  },
